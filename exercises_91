import javax.swing.JOptionPane;
public class Bender {
    public static void main(String[] args) {
       String fn = JOptionPane.showInputDialog("Введи первое число:");
       String sn = JOptionPane.showInputDialog("Введи второе число:");

       int num1 = Integer.parseInt(fn);
       int num2 = Integer.parseInt(sn);

       int sum = num1 + num2;
       JOptionPane.showInputDialog(null,"Сумма чисел равна" + sum, "Прибавляющий калькулятор", JOptionPane.PLAIN_MESSAGE);
    }
}
