interface AbilityToHunt{
    public void hunting();
}

public class Pro {
    public static void main(String[] args) {
        AbilityToHunt abilityToHunt = new AbilityToHunt(){
            @Override
            public void hunting(){
                System.out.println("Сезон охоты открыт!");
            }
        };
        abilityToHunt.hunting();
    }
}
