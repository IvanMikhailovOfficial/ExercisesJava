public class Bender {

        int sum(int a, int b, int c, int d){
         int result = a + b + c + d;
         return result;
        }

}

class Futur{
    public static void main(String[] args) {
        Bender bender = new Bender();
        int SumOfFourNumbers = bender.sum(5, 6, 7, 8);
        System.out.println(SumOfFourNumbers);
        System.out.println(bender.sum(9, 10, 11, 12));

    }
}
