public class Bender {
    public static void main(String[] args) {
        int x = 1;
        System.out.println("Перед началом цикла!");
        while (x <= 10){
            System.out.println("Внутри цикла!");
            System.out.println("Значение х равно " + x);
            x = x + 1;
        }
        System.out.println("Эта строка выполняется в любом случае!");
    }
}
