public class Pro {
    public static void main(String[] args) {
        int number = 8;
        int[] numbers = new int[4];// int[] numbers = {0,3,6,9};
        for(int i = 0; i < numbers.length; i = i + 1) {
            numbers[i] = i * 3;
        }
        for (int i = 0; i < numbers.length; i = i + 1){
            System.out.println(numbers[i]);
        }
    }
}
