public class Pro {
    public static void main(String[] args) {
        int x = 15;
        String c = "Cool";
        String space = " ";
        String name = "Bart";
        System.out.println(c + space + name);
        System.out.println("Cool" + " " + "Bart");
        System.out.println("Мой влзраст " + x + " лет");
    }
}
