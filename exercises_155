public class Pro {
    public static void main(String[] args) {
        Animal animal = new Animal();
        animal.eat();
        animal.destroy();
        animal.wool();
        animal.sleep();
        Cat cat = new Cat();
        cat.eat();
        cat.destroy();
        cat.wool();
        cat.sleep();
        cat.purr();
    }
}


public class Animal {
    public void eat(){
        System.out.println("Я кушаю!");
    }

    public void destroy(){
        System.out.println("Я порчу мебель!");
    }

    public void wool(){
        System.out.println("Я фабрика по производству шерсти!");
    }

    public void sleep(){
        System.out.println("Я сплю, не буди меня двуногий кожанный мешок!");
    }
}


public class Cat extends Animal{
    public void purr(){
        System.out.println("А еще я умею мырлыкать!");
    }

    public void sleep(){
        System.out.println("Спать я буду в твоей кровати!");
    }
}
