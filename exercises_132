public class Pro {
    public static void main(String[] args) {
        for (int i = 1; i <= 14; i = i + 2) {
            System.out.println("Прибавили 2 и получили значение " + i);
            System.out.println();
        }

        for (int i = 19; i >= 0; i = i - 5) {
                System.out.println("Убавляем 7 и получили значение " + i);
            System.out.println();
        }
    }
}
