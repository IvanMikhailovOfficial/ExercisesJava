public class Pro {
    public static void main(String[] args) {
        Client c1 = new Client("Гомер", 45);
        System.out.println(c1);
    }
}

class Client{
    private String name;
    private int age;

    public Client(String name, int age){
        this.name = name;
        this.age = age;
    }

    public String toString(){
        return name + ", " + age;
    }
}
