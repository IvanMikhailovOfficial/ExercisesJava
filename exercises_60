public class Bender {
    public static void main(String[] args) {
        int Beer = 9;

        if (Beer < 4)
        {
            System.out.println("Мне надо еще пива!");
        }
        else
        {
            if (Beer > 6)
            {
                System.out.println("Кажется я перебрал!");
            }
            System.out.println("Мне уже достаточно!");
        }
    }
}
